{
  "name": "scinapse",
  "version": "1.0.0",
  "description": "Academic search engine 'scinapse' web client repository",
  "sideEffects": false,
  "scripts": {
    "clean": "rimraf dist && rimraf scripts-dist",
    "start": "concurrently 'npm run clean' 'func start' 'npm:watch-*'",
    "watch-client": "NODE_ENV=local webpack-dev-server",
    "watch-server": "NODE_ENV=local webpack -w --config webpack.server.js",
    "build": "npm run clean && webpack && webpack --config webpack.server.js",
    "build:dev": "npm run clean && NODE_ENV=development webpack && NODE_ENV=development webpack --config webpack.server.js",
    "build:prod": "npm run clean && NODE_ENV=production webpack && NODE_ENV=production webpack --config webpack.server.js",
    "build-scripts": "webpack --config webpack.script.js && node scripts-dist/uploadToBlobStorage.js",
    "build-scripts:dev": "NODE_ENV=development webpack --config webpack.script.js && node scripts-dist/uploadToBlobStorage.js",
    "build-scripts:prod": "NODE_ENV=production webpack --config webpack.script.js && node scripts-dist/uploadToBlobStorage.js",
    "publish": "func azure functionapp publish scinapse-function",
    "publish:dev": "func azure functionapp publish scinapse-function --slot=dev",
    "deploy:dev": "npm run build:dev && npm run build-scripts:dev && npm run publish:dev",
    "deploy:prod": "npm run build:prod && npm run build-scripts:prod && npm run publish"
  },
