{
  "$schema": "http://json.schemastore.org/proxies",
  "proxies": {
    "api_server": {
      "matchCondition": {
        "methods": ["GET", "POST", "DELETE", "HEAD", "PATCH", "PUT", "OPTIONS", "TRACE"],
        "route": "/api/{*path}"
      },
      "backendUri": "https://api.scinapse.io/{path}"
    },
    "favicon": {
      "matchCondition": {
        "methods": ["GET"],
        "route": "/favicon.ico"
      },
      "backendUri": "https://assets.pluto.network/scinapse/favicon.ico"
    }
  }
}
