{
  "$schema": "http://json.schemastore.org/proxies",
  "proxies": {
    "api_server": {
      "matchCondition": {
        "methods": [
          "GET",
          "POST",
          "DELETE",
          "HEAD",
          "PATCH",
          "PUT",
          "OPTIONS",
          "TRACE"
        ],
        "route": "/api/{*path}"
      },
      "backendUri": "https://dev-api.scinapse.io/{path}"
    },
    "assets": {
      "matchCondition": {
        "methods": ["GET"],
        "route": "/assets/{*path}"
      },
      "backendUri": "https://scinapsefunction.blob.core.windows.net/client-assets/{path}"
    }
  }
}
